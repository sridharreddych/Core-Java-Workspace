package com.sree.templatepattern;

public class Cricket extends Game {
	@Override
	void endPlay() {
		System.out.println("Cricket Gam e Finished!");
	}

	@Override
	void initialize() {
		System.out.println("Cricket Gam e Initialized! Start playing.");
	}

	@Override
	void startPlay() {
		System.out.println("Cricket Gam e Started. Enjoy the gam e!");
	}
}